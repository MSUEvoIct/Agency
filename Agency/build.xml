<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<project basedir="." default="build" name="Agency">

 <path id="compile.classpath">
  <fileset dir="lib">
   <include name="*.jar"/>
  </fileset>
 </path>

 <target name="build">
  <javac srcdir="src" destdir="bin" debug="on">
   <classpath refid="compile.classpath"/>
  </javac>
 </target>

</project>
